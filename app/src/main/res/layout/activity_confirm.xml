<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/login_main_title_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="60dp"
        android:textSize="20dp"
        android:text="Datos"
        android:gravity="left"
        android:layout_marginLeft="30dp"
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/Sevicio_Et"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        android:layout_marginTop="90dp"
        app:layout_constraintTop_toBottomOf="@+id/login_main_title_name"
        android:text="Servicio..."
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/estilista_Et"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        android:layout_marginTop="40dp"
        app:layout_constraintTop_toBottomOf="@+id/Sevicio_Et"
        android:text="Estilista..."
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/Fecha_Et"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        android:layout_marginTop="40dp"
        app:layout_constraintTop_toBottomOf="@+id/estilista_Et"
        android:text="Fecha..."
        />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/Hora_Et"
        android:layout_width="match_parent"

        android:layout_height="wrap_content"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        android:layout_marginTop="40dp"
        app:layout_constraintTop_toBottomOf="@+id/Fecha_Et"
        android:text="Fecha..."
        />



    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/login_sign_in_button"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/Hora_Et"
        android:layout_marginStart="30dp"
        android:layout_marginEnd="30dp"
        android:layout_marginTop="100dp"
        app:layout_constraintBottom_toTopOf="@+id/login_bottom_caption"
        android:text="Confirmar"
        />


</androidx.constraintlayout.widget.ConstraintLayout>
